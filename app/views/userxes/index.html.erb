<p id="notice"><%= notice %></p>

<h1>Listing Userxes</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @userxes.each do |userx| %>
      <tr>
        <td><%= link_to 'Show', userx %></td>
        <td><%= link_to 'Edit', edit_userx_path(userx) %></td>
        <td><%= link_to 'Destroy', userx, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Userx', new_userx_path %>
